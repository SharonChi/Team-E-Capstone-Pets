@model IEnumerable<test.TMethod>

@{
    ViewBag.Title = "Index";
}

<h2>Medication Method Report</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<div class="body-content">


	<table class="table">
		<tr>
			<th>
				Form
			</th>
			<th></th>
		</tr>

		@foreach (var item in Model) {
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.strMethod)
				</td>
				<td>
					@Html.ActionLink("Edit", "Edit", new { id = item.intMethodID }, new { @class = "link-icon-edit" }) 
				</td>
				<td>
					@Html.ActionLink("Details", "Details", new { id = item.intMethodID }, new { @class = "link-icon-details" }) 
				</td>
				<td>
					@Html.ActionLink("Delete", "Delete", new { id = item.intMethodID }, new { @class = "link-icon-delete" })
				</td>
			</tr>
		}

	</table>
</div>
